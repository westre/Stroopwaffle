<UserControl x:Class="Stroopwaffle_Launcher.ServerEntry"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Stroopwaffle_Launcher"
             mc:Ignorable="d" Height="34.638">
    <Grid Width="935" Height="29">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="43*"/>
            <ColumnDefinition Width="435*"/>
            <ColumnDefinition Width="96*"/>
            <ColumnDefinition Width="89*"/>
            <ColumnDefinition Width="272*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="lblName" Content="Button" Grid.Column="1"/>
        <Label x:Name="lblPlayers" Content="Label" Grid.Column="2"/>
        <Label x:Name="lblPing" Content="Label" Grid.Column="3"/>
        <Label x:Name="lblScript" Content="Label" Grid.Column="4"/>
        <Label x:Name="lblVersion" Content="Label"/>
    </Grid>
</UserControl>
